<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.allan.test">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashViewActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:screenOrientation="portrait" />

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/actions" />

        <activity
            android:name=".CadastroActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BottomNavigationActivity"
            android:label="@string/title_activity_home"
            android:screenOrientation="portrait"/>
        <activity android:name=".EditarExcluirUsuarioActivity" android:screenOrientation="portrait"/>
        <activity android:name=".CadastroDisciplinasActivity" android:screenOrientation="portrait"></activity>
    </application>

</manifest>